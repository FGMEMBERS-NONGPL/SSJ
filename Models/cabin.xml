<PropertyList>

    <path>cabin.ac</path>

<animation>
      <type>material</type>
      <object-name>ceiling</object-name>
       
      <condition>
             <less-than-equals>	
	      <property>/sim[0]/time/CNANode</property>
	      <value>100</value> 
	    
	       <!-- 
	       <property>/sim/time/sun-angle-rad</property>
               <value>1.57</value> -->
      
	      </less-than-equals>	
      </condition>
      <emission>
         <red>1</red>
         <green>1</green>
         <blue>1</blue>
      </emission>
   </animation>
   
<animation>
      <type>material</type>
      <object-name>ceiling</object-name>
       
      <condition>
             <greater-than>	
	      <property>/sim[0]/time/CNANode</property>
	      <value>100</value> 
	    
	       <!-- 
	       <property>/sim/time/sun-angle-rad</property>
               <value>1.57</value> -->
      
	      </greater-than>	
      </condition>
      <emission>
         <red>1</red>
         <green>1</green>
         <blue>1</blue>
      </emission>
   </animation>
   
   

<animation>
      <type>material</type>
      <object-name>overhead cabinetsL</object-name>
      <object-name>overhead cabinetsR</object-name>
       <condition>
	 <and>
           <less-than>
	      <property>/instrumentation[0]/altimeter/indicated-altitude-ft</property>
              <value>800</value>
          </less-than>
	<and>
	  <greater-than>
	      <property>/instrumentation[0]/airspeed-indicator[0]/indicated-speed-kt</property>
              <value>40</value>
          </greater-than>
	  
	</and>  
	  </and>  
	 </condition>
      <emission>
         <red>0.5</red>
         <green>0.5</green>
         <blue>0.5</blue>
      </emission>
   </animation>



<animation>
      <type>material</type>
      <object-name>TopSeats</object-name>
